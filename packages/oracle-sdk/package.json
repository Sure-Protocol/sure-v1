{
	"name": "@surec/oracle",
	"version": "1.0.3",
	"type": "module",
	"description": "Typescript SDK for the Sure Oracle to be used to bring off-chain data on-chain",
	"keywords": [
		"solana",
		"oracle",
		"sure",
		"sdk"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://github.com/Sure-Protocol/sure-v1",
	"files": [
		"dist/",
		"src/"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"packageManager": "yarn@3.2.1",
	"scripts": {
		"build": "tsc --p tsconfig.json && tsc --p tsconfig.cjs.json",
		"build:oracleSdk": "yarn build",
		"setup": "yarn",
		"build:esm": "tsc --p tsconfig.json",
		"build:cjs": "tsc --p tsconfig.cjs.json",
		"build:up": "rollup --config",
		"idl:update": "cp ./../../target/types/oracle.ts ./../idls/oracle.ts"
	},
	"devDependencies": {
		"@project-serum/anchor": "^0.25.0",
		"@saberhq/solana-contrib": "^1.14.5",
		"@saberhq/tsconfig": "^2.0.4",
		"@solana/spl-token": "^0.3.4",
		"@solana/web3.js": "^1.54.1",
		"@types/randombytes": "^2.0.0",
		"bn.js": "^5.2.1",
		"randombytes": "^2.1.0",
		"typescript": "^4.7.4"
	},
	"gitHead": "628105ff8e1ab435ab513ecc72ff032d6fe5cbc3",
	"dependencies": {
		"@saberhq/anchor-contrib": "^1.14.5"
	}
}
