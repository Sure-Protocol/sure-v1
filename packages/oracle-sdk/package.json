{
	"name": "@surec/oracle",
	"version": "0.0.18",
	"description": "Typescript SDK for the Sure Oracle to be used to bring off-chain data on-chain",
	"keywords": [
		"solana",
		"oracle",
		"sure",
		"sdk"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"repository": "https://github.com/Sure-Protocol/sure-v1",
	"exports": {
		".": {
			"import": "./dist/esm/oracle-sdk/index.js",
			"require": "./dist/cjs/oracle-sdk/index.js"
		}
	},
	"main": "dist/cjs/oracle-sdk/index.js",
	"module": "dist/esm/oracle-sdk/index.js",
	"packageManager": "yarn@3.2.1",
	"dependencies": {
		"@project-serum/anchor": "^0.25.0",
		"@saberhq/anchor-contrib": "^1.14.2",
		"@saberhq/solana-contrib": "^1.14.2",
		"@saberhq/token-utils": "^1.14.2",
		"@solana/spl-token": "^0.3.2",
		"@solana/web3.js": "^1.53.0"
	},
	"scripts": {
		"build": "tsc --p tsconfig.json && tsc --p tsconfig.esm.json"
	},
	"devDependencies": {
		"@saberhq/tsconfig": "^2.0.4",
		"typescript": "^4.7.4"
	},
	"gitHead": "4ae0179911cff99b7d62679a736ac15fc1b42a58"
}
